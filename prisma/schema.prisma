generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Taxi {
  id Int @id @default(autoincrement())
  plate String 
  @@map("taxis")
}

model Trajectory {
  id Int @id @default(autoincrement())
  taxi_id Int
  @@map("trajectories")
}
